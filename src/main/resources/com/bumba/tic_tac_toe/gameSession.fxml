<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox prefWidth="490.0" spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bumba.tic_tac_toe.LobbyController">
    <Label fx:id="IDLabel" prefHeight="26.0" prefWidth="150.0">
      <HBox.margin>
         <Insets right="20.0" />
      </HBox.margin></Label>
    <Label fx:id="statusLabel">
      <HBox.margin>
         <Insets right="50.0" />
      </HBox.margin></Label>
    <Button fx:id="joinGameButton" onAction="#joinGame" text="Join">
      <HBox.margin>
         <Insets left="50.0" right="25.0" />
      </HBox.margin></Button>
    <Button fx:id="spectateButton" onAction="#spectate" text="Spectate">
      <HBox.margin>
         <Insets left="25.0" />
      </HBox.margin></Button>
</HBox>
